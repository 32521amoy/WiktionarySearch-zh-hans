{
  "name": "__MSG_name__",
  "version": "1.1.2",
  "manifest_version": 2,
  "description": "__MSG_desc__",
  "homepage_url": "https://github.com/ZauberViolino/WiktionarySearch-zh-hans/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "zh",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Wiktionary Search zh-hans",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "contextMenus",
    "https://zh.wiktionary.org/zh-hans/*",
		"storage"
  ]
}
